library(readxl)
library(dplyr)
library(factoextra)
library(FactoMineR)
DATA_PROJECT_1_2022_ <- read_excel("C:/Users/hanfai/portfolio/peubah ganda/DATA PROJECT.xlsx")
table(is.na(DATA_PROJECT_1_2022_)) # No NA
head(DATA_PROJECT_1_2022_)
str(DATA_PROJECT_1_2022_)
summary(DATA_PROJECT_1_2022_[,6:13])
project.filtered = DATA_PROJECT_1_2022_ %>%
filter(nama_provinsi == "JAWA BARAT") %>%
group_by(nama_kota) %>%
summarize(mean(standar_isi),mean(standar_proses),mean(standar_kompetensi),mean(standar_pendidik),
mean(standar_sarana),mean(standar_pengelolaan),mean(standar_biaya),mean(standar_nilai))
project.filtered2 = project.filtered[,-1]
View(project.filtered2)
#Kmeans
fviz_nbclust(project.filtered2, FUNcluster = hcut, method = "wss")
project.kmeans <- eclust(project.filtered2, stand = F, FUNcluster = "kmeans",
k=2, graph=T)
project.kmeans$cluster
project.kmeans$centers
aggregate(project.filtered2, by=list(gerombol=project.kmeans$cluster), FUN=mean)
#Kmeans alternative
project.kmeans2 = kmeans(project.filtered2, centers = 2, nstart = 20)
fviz_cluster(project.kmeans2, project.filtered2)
project.pca = prcomp(project.filtered2)
project.pca2 = PCA(project.filtered2,graph = F)
project.pca$rotation
project.pca2$eig
fviz_pca_biplot(project.pca)
#Coba di scale
project.kmeans3 <- eclust(project.filtered2, stand = T, FUNcluster = "kmeans",
k=2, graph=T)
project.kmeans3$cluster
project.kmeans3$centers
aggregate(project.filtered2, by=list(gerombol=project.kmeans3$cluster), FUN=mean)
project.pca3 = prcomp(project.filtered2)
project.pca3$rotation
fviz_pca_biplot(project.pca3)
